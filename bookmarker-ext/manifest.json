{
  "manifest_version": 2,

  "name": "BookMarklet",
  "description": "Extension add the current page to your localstorage",
  "version": "1.0.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "bookmarklet"
  },
  "permissions": [
    "activeTab"
  ]
}
